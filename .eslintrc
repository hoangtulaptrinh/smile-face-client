{
  "extends": ["react-app", "plugin:prettier/recommended"],
  "env": {
    "es6": true,
    "jest": true,
    "browser": true
  },
  "globals": {
    "process": true
  },
  "rules": {
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
    "semi": 1
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019
  }
}
